				<ConceptTemplate uuid="06e51d50-a53a-4602-bc33-8c1a334f56d0" name="NOTDEFINED" applicableSchema="IFC4" applicableEntity="IfcElementAssembly" />
				<ConceptTemplate uuid="17cf9f91-0b80-4dda-9d83-71f2a1c95a3c" name="NOTDEFINED" applicableSchema="IFC4" applicableEntity="IfcElementAssembly" />
